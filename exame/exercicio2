# escreva o valor de x
x = 1
# coloque o laço de repetição
for x in range(10):
    # incrementa o x
    x = x + 2
    # peça o valor de y
    y = int(input("digite o valor de y(entre 1 a 10):"))
    # se y for igual a 5
    if y == 5:
        # mostre "parou"
        print("PAROU")
        break
    # mostre o x
    print(x)
